}

QCheckBox::indicator {
    border: 2px solid #CCCCCC; /* Borde suave del indicador */
    border-radius: 3px; /* Bordes redondeados para el indicador */
    background-color: white; /* Fondo blanco */
    width: 12px; /* Tamaño del indicador */
    height: 12px; /* Tamaño del indicador */
    transition: all 0.3s ease; /* Transición suave */
}

/* Cuando el checkbox está marcado */
QCheckBox::indicator:checked {
    background-color: #16344D; /* Color de fondo cuando está marcado */
    border: 2px solid #16344D; /* Borde del indicador en azul */
}

/* Efecto cuando el mouse pasa por encima */
QCheckBox:hover {
    background-color: #f0f0f0; /* Fondo más claro al pasar el mouse */
    border-bottom: 2px solid #16344D; /* Engrosa solo el borde inferior */
}
