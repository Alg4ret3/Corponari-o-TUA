             <string notr="true">QTableWidget {
    background-color: #f5f5f5; /* Fondo gris claro */
    border: 1px solid #CCCCCC; /* Borde suave */
    border-radius: 2px; /* Bordes redondeados */
    font-size: 12px; /* Tamaño de fuente */
    padding: 5px; /* Espaciado interno */
    gridline-color: #DDDDDD; /* Color de las líneas de la cuadrícula */
}

QTableWidget::item {
    padding: 5px; /* Espaciado de las celdas */
    border-radius: 2px; /* Bordes redondeados en las celdas */
}

QTableWidget::item:hover {
    background-color: #f0f0f0; /* Fondo claro al pasar el mouse */
    color: #16344D; /* Texto azul al pasar el mouse */
}

/* Barra de desplazamiento vertical y horizontal */
QScrollBar:vertical, QScrollBar:horizontal {
	
    border-radius: 15px; /* Bordes redondeados */
    background-color: #f5f5f5; /* Fondo gris claro */
    width: 7px; /* Ancho de la barra vertical */
    height: 7px; /* Alto de la barra horizontal */
}

/* Manejador de la barra de desplazamiento (el &quot;handle&quot;) */
QScrollBar::handle:vertical, QScrollBar::handle:horizontal {
    background-color: #16344D; /* Barra de desplazamiento en azul */
    border-radius: 15px; /* Bordes redondeados */
    min-height: 10px; /* Tamaño mínimo de la barra de desplazamiento vertical */
    min-width: 10px; /* Tamaño mínimo de la barra de desplazamiento horizontal */
}

/* Efecto cuando el mouse pasa por encima del manejador */
QScrollBar::handle:vertical:hover, QScrollBar::handle:horizontal:hover {
    background-color: #1A4567; /* Fondo más oscuro al pasar el mouse */
}

/* Líneas de los botones de desplazamiento (arriba/abajo o izquierda/derecha) */
QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical,
QScrollBar::add-line:horizontal, QScrollBar::sub-line:horizontal {
    background: none; /* Quitar los botones de desplazamiento */
}

/* Flechas de los botones de desplazamiento */
QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical,
QScrollBar::left-arrow:horizontal, QScrollBar::right-arrow:horizontal {
    background: none; /* Quitar las flechas */
}
